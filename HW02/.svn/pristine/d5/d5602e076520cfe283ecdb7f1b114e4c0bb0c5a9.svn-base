        <ownedAttribute xmi:id="_Jrq6QTfZEeSu1vxWufepuQ" name="model" visibility="private" type="_JqhqwDfZEeSu1vxWufepuQ" association="_JsA4hDfZEeSu1vxWufepuQ"/>
