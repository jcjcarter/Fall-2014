        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Jr3HjzfZEeSu1vxWufepuQ" name="Controller" specification="_JqfOgDfZEeSu1vxWufepuQ">
          <ownedParameter xmi:id="_Jr3HkDfZEeSu1vxWufepuQ" type="_JqencDfZEeSu1vxWufepuQ" direction="return"/>
          <language>java</language>
          <body>/* Instantiates a new instance of BallModel as model */
		model = new BallModel(new IModel2ViewAdapter() {

			@Override
			public void update() {
				view.update();
			}

		});

		/* Instantiates a new instance of BallView as view */
		view = new BallView(new IView2ModelAdapter() {

			@Override
			public void paint(Graphics g) {
				model.update(g);
			}

			@Override
			public void clearBalls() {
				model.clearBalls();
			}

			@Override
			public void addBall(String className, Component canvas) {
				ABall ball = model.loadBall(className, canvas);
				if (ball != null) {
					model.addSprite(ball);
				}
			}

		});
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_Jr3ukDfZEeSu1vxWufepuQ" name="start" specification="_JqfOhDfZEeSu1vxWufepuQ">
